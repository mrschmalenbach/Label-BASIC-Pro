REM good_if_nesting.lbpREM Simple, well-formed nested IF / ELSE / END IF testsREM ##OPTION STRICT_ERR ONX% = 3Y% = 7PRINT "STARTING NESTED IF TEST..."PRINT "X=";X%;" Y=";Y%REM Top-level IF with ELSE, and nested IF insideIF X%<Y% THEN  PRINT "BRANCH: X<Y"  IF X%>0 THEN    PRINT "  NESTED: X>0"  END IFELSE  PRINT "BRANCH: X>=Y"END IFREM A second independent IF with no ELSEIF X%=3 THEN  PRINT "SECOND IF: X IS THREE"END IFREM A third IF with a different conditionIF Y%>10 THEN  PRINT "THIRD IF: Y>10"ELSE  PRINT "THIRD IF: Y<=10"END IFPRINT "NESTED IF TEST COMPLETE."